{
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": false,
    "ban-types": false,
    "callable-types": false,
    "interface-name": [true, "never-prefix"],
    "jsdoc-format": false,
    "max-classes-per-file": false,
    "no-console": [true, "log"],
    "no-var-requires": false,
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"]
  }
}
