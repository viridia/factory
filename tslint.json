{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "arrow-parens": false,
    "interface-name": [true, "never-prefix"],
    "jsdoc-format": false,
    "jsx-no-multiline-js": false,
    "no-console": [true, "log"],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"]
  }
}
